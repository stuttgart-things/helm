---
kubeProxyReplacement: true
routingMode: "native"
ipv4NativeRoutingCIDR: "10.244.0.0/16"
k8sServiceHost: "helm-dev-control-plane"
k8sServicePort: 6443

l2announcements:
  enabled: true
  leaseDuration: "3s"
  leaseRenewDeadline: "1s"
  leaseRetryPeriod: "500ms"

devices: ["eth0", "net0"]

externalIPs:
  enabled: true

autoDirectNodeRoutes: true

operator:
  replicas: 2